<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhang.test">

    <uses-permission android:name="android.permission.SET_WALLPAPER"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.zhang.test.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.zhang.test.MainActivity">
            <intent-filter>
                <action android:name="com.example.zhang.test.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.zhang.test.Menu">
            <intent-filter>
                <action android:name="com.example.zhang.test.MENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.zhang.test.TextPlay">
        </activity>
        <activity android:name="com.example.zhang.test.Email">
        </activity>
        <activity
            android:name="com.example.zhang.test.Camera"
            android:label="Camera and Wallpaper"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.example.zhang.test.Data">
        </activity>
        <activity android:name="com.example.zhang.test.OpenedClass">
        </activity>
        <activity
            android:name="com.example.zhang.test.AboutUs"
            android:theme="@android:style/Theme.Dialog">
            <intent-filter>
                <action android:name="com.example.zhang.test.ABOUT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.zhang.test.Prefs"
            android:theme="@android:style/Theme.Dialog">
            <intent-filter>
                <action android:name="com.example.zhang.test.PREFS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.zhang.test.GFX">
        </activity>
        <activity android:name="com.example.zhang.test.GFXSurface">
        </activity>
        <activity android:name="com.example.zhang.test.SoundStuff">
        </activity>
        <activity android:name="com.example.zhang.test.Slider">
        </activity>
        <activity android:name="com.example.zhang.test.Tabs">
        </activity>
        <activity android:name="com.example.zhang.test.SimpleBrowser">
        </activity>
        <activity android:name="com.example.zhang.test.Flipper">
        </activity>
        <activity android:name="com.example.zhang.test.SharedPrefs">
        </activity>
        <activity android:name="com.example.zhang.test.InternalData">
        </activity>
        <activity android:name="com.example.zhang.test.ExternalData">
        </activity>
        <activity android:name="com.example.zhang.test.SQLiteExample">
        </activity>
        <activity android:name="com.example.zhang.test.SQLView">
            <intent-filter>
                <action android:name="com.example.zhang.test.SQLVIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>